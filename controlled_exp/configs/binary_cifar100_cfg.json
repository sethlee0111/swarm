{
    "dataset": "cifar100",
    "intervals": [200],
    "repeat": 1,
    "local-set": [0,1],
    "goal-set": [0,1],
    "local-task": "porcupine-poppy",
    "goal-tasks": "medium_mammals-flowers",
    "task-encounters": [
        ["porcupine-poppy","possum-rose", "raccoon-sunflower", "bear-maple_tree", "leopard-oak_tree", "bed-bridge", "chair-castle"]
    ],
    "temp": [["fox-orchid", "porcupine-poppy", "possum-rose", "raccoon-sunflower"],
        ["bear-maple_tree", "leopard-oak_tree"],
        ["aquarium_fish-apple", "flatfish-mushroom"]
        ],
    "temp2":
        [
            ["fox-orchid", "porcupine-poppy", "possum-rose"],
            ["bear-maple_tree", "leopard-oak_tree", "lion-palm_tree"],
            ["bed-bridge", "chair-castle"]
        ],
    "noise-label-set-size": 2,
    "same-repeat": false,
    "number-of-data-points": 200,
    "noise-percentage": 100,
    "strategies": {
        "greedy ": true,
        "oracle ": true,
        "opportunistic": false,
        "gradient replay": false,
        "task-aware": true,
        "compressed": false,
        "compressed-v2": false
    },
    "pretrained-model": "../pretrained/big_bin_cnn_mammals_vs_flowers4.pickle",
    "hyperparams" : {
        "orig-lr": 0.00005,
        "batch-size": 100,
        "kappa": 7,
        "offset": 0.96,
        "apply-rate": 3,
        "test-data-per-label": 100,
        "evaluation-metrics": "loss-and-accuracy",
        "similarity-threshold": 0.2,
        "task-threshold": 1.4
    },
    "test-data-per-label": 400
}